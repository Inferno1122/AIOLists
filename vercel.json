{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/favicon.ico",
      "destination": "/assets/logo.ico"
    },
    {
      "source": "/configure",
      "destination": "/index.html"
    },
    {
      "source": "/configure/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/index.html",
      "destination": "/index.html"
    },
    {
      "source": "/(manifest)(?:\\?.*)?$",
      "destination": "/src/server.js"
    },
    {
      "source": "/(catalog|search|meta)/:path*",
      "destination": "/src/server.js"
    }
  ]
}
